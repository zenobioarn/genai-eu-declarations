{
  "name": "Microsoft Copilot",
  "terms": {
    "Terms of Service": {
      "fetch": "https://www.microsoft.com/en-us/microsoft-copilot/for-individuals/termsofuse",
      "select": [
        "#mainContent"
      ],
      "remove": [
        ".cdx_mkt_brand"
      ],
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://privacy.microsoft.com/en-ie/privacystatement",
      "select": [
        "main"
      ],
      "remove": [
        "nav"
      ]
    },
    "Acceptable Use Policy": {
      "fetch": "https://www.microsoft.com/en-gb/servicesagreement/",
      "select": [
        "main"
      ],
      "remove": [
        "div.page[id$=\"XboxLive\"]"
      ],
      "executeClientScripts": true
    }
  }
}
