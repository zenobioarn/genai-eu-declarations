{
  "@opentermsarchive/engine": {
    "recorder": {
      "versions": {
        "storage": {
          "git": {
            "snapshotIdentiferTemplate": "https://github.com/OpenTermsArchive/genai-eu-snapshots/commit/%SNAPSHOT_ID",
            "repository": "git@github.com:OpenTermsArchive/genai-eu-versions.git",
            "path": "./data/versions"
          }
        }
      },
      "snapshots": {
        "storage": {
          "git": {
            "repository": "git@github.com:OpenTermsArchive/genai-eu-snapshots.git",
            "path": "./data/snapshots"
          }
        }
      }
    },
    "fetcher": {
      "language": "en"
    },
    "notifier": {
      "sendInBlue": {
        "updatesListId": 0,
        "updateTemplateId": 0
      }
    },
    "reporter": {
      "type": "github",
      "repositories": {
        "declarations": "OpenTermsArchive/genai-eu-declarations",
        "versions": "OpenTermsArchive/genai-eu-versions",
        "snapshots": "OpenTermsArchive/genai-eu-snapshots"
      }
    },
    "dataset": {
      "title": "genai-eu",
      "versionsRepositoryURL": "https://github.com/OpenTermsArchive/genai-eu-versions"
    },
    "collection-api": {
      "port": 3000,
      "basePath": "/collection-api"
    },
    "logger": {
      "timestampPrefix": false
    }
  }
}
